<UserControl x:Class="ClientUI.Menu"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="720" Width="100" Background="Gold" BorderBrush="Black">
    <UserControl.Resources>
        <ResourceDictionary Source="MenuButtonStyle.xaml" />
    </UserControl.Resources>
    <StackPanel>
        <Button Style="{StaticResource ButtonMenu}" Height="35" Width="90" Background="CornflowerBlue" >Widok miesiąca</Button>
        <Button Style="{StaticResource ButtonMenu}" Height="35" Width="90" Background="CornflowerBlue">Widok tygodnia</Button>
        <Button Style="{StaticResource ButtonMenu}" Height="35" Width="90" Background="CornflowerBlue">Widok dnia</Button>
        
    </StackPanel>
</UserControl>
